<form class="form-inline" action="vmrc://@[HOST]:[PORT]/?moid=[VM-MOREF]" method="get">
  <div class="form-group">

		<select class="form-control">
  		<% gateways.each do |gateway| %>
	  		<option value=<%= gateway[:name] %>> <%= "#{gateway[:name]} (#{gateway[:vdc]})" %> </option>
		  <% end %>
		</select>
  </div>
  <button type="submit" class="btn btn-default">Launch Console</button>
</form>






